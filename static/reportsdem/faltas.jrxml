     tblresumen."mpcodigo" AS tblresumen_mpcodigo,
     tblresumen."fecha" AS tblresumen_fecha,
     tblresumen."numtimbradas" AS tblresumen_numtimbradas,
     tblresumen."hora1" AS tblresumen_hora1,
