	<queryString>
		<![CDATA[SELECT tbleventos.id AS tbleventos_id,
	tbleventos_lugares.nombre AS tbleventos_lugares_nombre,
	tbleventos_lugares.ciudad AS tbleventos_lugares_ciudad,
	tbleventos_lugares.direccion AS tbleventos_lugares_direccion,
	tbleventos_lugares.nombre AS tbleventos_lugares_nombre,
	tbleventos_lugares.ciudad AS tbleventos_lugares_ciudad,
	tbleventos_lugares.direccion AS tbleventos_lugares_direccion,
	tbleventos.nombre AS tbleventos_nombre,
	tbleventos.descripcion AS tbleventos_descripcion,
	tbleventos.fechainicial AS tbleventos_fechainicial,
	tbleventos.horainicial AS tbleventos_horainicial,
	tblparcontasist.razonsocial AS tblparcontasist_razonsocial
FROM dbo.tbleventos tbleventos
	INNER JOIN dbo.tbleventos_lugares tbleventos_lugares ON
	 tbleventos.evlugcodigo = tbleventos_lugares.id ,
	dbo.tblparcontasist tblparcontasist
	WHERE 1=1 $P!{filtro}]]>
